{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "jsx-a11y/href-no-hash": "off",
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4]
    },
    "globals": {
        "it": false
    }
}
